    "gulp-htmlmin": "*",
    "gulp-iconfont": "*",
    "gulp-iconfont-css": "*",
    "gulp-if": "*",
    "gulp-jade": "^1.1.0",
    "gulp-minify-css": "*",
    "gulp-minify-html": "*",
    "gulp-pxtorem": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-rimraf": "^0.2.1",
    "gulp-sass": "*",
    "gulp-using": "*",
    "gulp-util": "*",
    "kss": "^2.4.0",
    "node-sass": "^4.4.0",
    "run-sequence": "^1.2.2"
  },
  "dependencies": {
    "balloon-css": "^0.5.0",
    "bootstrap": "^4.0.0",
    "clipboard-js": "^0.3.5",
